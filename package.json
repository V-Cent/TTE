{
  "name": "tte",
  "version": "0.15.6",
  "description": "Tales Tech Encyclopedia",
  "main": "src/documentParser.js",
  "private": true,
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "docs/"
    ],
    "ext": "js,html,css"
  },
  "scripts": {
    "watch": "nodemon --watch \"src/\" --exec \"npm run build\"",
    "build": "node esbuild.mjs && minify-html --output docs/index.html --keep-closing-tags --minify-css src/index.html"
  },
  "dependencies": {
    "@minify-html/node": "^0.15.0",
    "esbuild": "^0.23.1",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-ignore": "^1.1.1",
    "katex": "^0.16.11",
    "nodemon": "^3.1.4",
    "remarkable": "^2.0.1",
    "remarkable-katex": "^1.2.1"
  }
}
